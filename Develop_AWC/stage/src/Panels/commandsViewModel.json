            "id": "FirstPanel",
            "uiAnchor": "aw_rightWall",
            "priority": 100
        }
    },
    "actions": {
        "activateFirstPanel": {
            "actionType": "JSFunction",
            "method": "activateCommandPanel",
            "deps": "js/commandPanel.service",
            "inputData": {
                "commandId": "FirstPanel",
                "location": "aw_toolsAndInfo"
            }
        }
    },
    "conditions": {
        "objectIsSelected": {
            "expression": "ctx.selected"
        }
    },
    "i18n": {
        "FirstPanelTitle": [
            "PanelsMessages"
        ],
        "FirstPanelDescription": [
            "PanelsMessages"
        ]
