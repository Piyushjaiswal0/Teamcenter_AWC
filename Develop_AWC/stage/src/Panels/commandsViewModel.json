{
    "commands": {
        "FirstPanel": {
            "iconId": "cmdWalk",
            "title": "{{i18n.FirstPanelTitle}}",
            "description": "{{i18n.FirstPanelDescription}}"
        }
    },
    "commandHandlers": {
        "FirstPanelHandler": {
            "id": "FirstPanel",
            "action": "activateFirstPanel",
            "activeWhen": true,
            "visibleWhen": {
